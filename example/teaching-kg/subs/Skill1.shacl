                <http://mapping.example.com/map_knowledge_topic_3_000/shape1915> ;
            sh:nodeKind sh:IRI ;
            sh:path schema1:teaches ],
        [ rdfs:label "requiresKnowledge"@en ;
            sh:class educor:KnowledgeTopic ;
